{
  "name": "vscode-env0",
  "publisher": "env0",
  "displayName": "env0",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "env0.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "env0.openInEnv0",
        "title": "Open in env0",
        "icon": "resources/link-icon.svg"
      },
      {
        "command": "env0.redeploy",
        "title": "Redeploy",
        "icon": "resources/redeploy.svg"
      },
      {
        "command": "env0.abort",
        "title": "Abort",
        "icon": "resources/abort.svg"
      },
      {
        "command": "env0.destroy",
        "title": "Destroy",
        "icon": "resources/destroy.svg"
      },
      {
        "command": "env0.approve",
        "title": "Approve",
        "icon": "resources/approve.svg"
      },
      {
        "command": "env0.cancel",
        "title": "Cancel",
        "icon": "resources/cancel.svg"
      }
    ],
    "configuration": {
      "title": "env0",
      "properties": {
        "env0.apiKeyId": {
          "type": "string",
          "required": true,
          "description": "env0 API key ID"
        },
        "env0.secretKey": {
          "type": "string",
          "required": true,
          "description": "env0 API secret key"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "env0",
          "title": "env0",
          "icon": "resources/env0-icon.svg"
        }
      ]
    },
    "views": {
      "env0": [
        {
          "id": "env0-environments",
          "name": "Environments"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "env0.openInEnv0",
          "when": "view == env0-environments",
          "group": "inline"
        },
        {
          "command": "env0.redeploy",
          "when": "view == env0-environments && viewItem != IN_PROGRESS && viewItem != WAITING_FOR_USER",
          "group": "inline"
        },
        {
          "command": "env0.abort",
          "when": "view == env0-environments && viewItem == IN_PROGRESS && viewItem != WAITING_FOR_USER",
          "group": "inline"
        },
        {
          "command": "env0.approve",
          "when": "view == env0-environments && viewItem == WAITING_FOR_USER",
          "group": "inline"
        },
        {
          "command": "env0.cancel",
          "when": "view == env0-environments && viewItem == WAITING_FOR_USER",
          "group": "inline"
        },
        {
          "command": "env0.destroy",
          "when": "view == env0-environments",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^0.27.2",
    "strip-ansi": "^7.0.1"
  }
}